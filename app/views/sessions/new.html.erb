<h1>Log in</h1>

<%= form_tag sessions_path do %>
  <%=label_tag 'Email'%>
  <%= text_field_tag :email %>

<%=label_tag 'Password'%>
 <%= password_field_tag :password, nil, class: 'masked_input_field' %>
  <%= submit_tag  "Log in" %>
<p>New user? <%= link_to "Sign up now!", new_user_path(@user) %></p>
<% end %>


