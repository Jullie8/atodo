
<%= render 'shared/header' %>

<h1>Welcome blah<h1>


<ul>
  <div class="container">
    <% @current_user_todos.each do |todo| %>
      <div class="square">
      <li>  <%= todo.title %> </li>
      </div>
    <% end %>
  </div>
</ul>

<h1>Create a New Todo </h1>

<%= form_for Todo.new do |f| %>
<%= f.label :title  %>
<%= f.text_field :title %>
<%= f.submit 'Add New Todo'%>
<% end %>
